    <text id="de"><span class="nWarn">$appName konnte nicht geladen werden.</span></text>
    <text id="hr"><span class="nWarn">Failed to load $appName.</span></text>
  </item>

  <item id="UserLogin">
    <text id="en-US"><span class="nOk">$userName logged in.</span></text>
    <text id="de"><span class="nOk">$userName hat sich angemeldet.</span></text>
    <text id="hr"><span class="nOk">$userName prijavljeni.</span></text>
  </item>
  <item id="UserLogout">
    <text id="en-US"><span class="nOk">$userName logged out.</span></text>
    <text id="de"><span class="nOk">$userName hat sich abgemeldet.</span></text>
    <text id="hr"><span class="nOk">$userName odjavljeni.</span></text>
  </item>

  <item id="VodBuyMovieFP">
    <text id="en-US"><span class="nNotify">The movie costs $price.</span><br/><span class="nInfoText">Please confirm your order.</span></text>
    <text id="de"><span class="nNotify">Der Film $movieName kostet $price.</span><br/><p class="nInfoText">Bitte bestätigen Sie Ihre Bestellung.</p></text>
    <text id="hr"><span class="nNotify">Film iznosi $price.</span><br/><p class="nInfoText">Molimo vas da potvrdite svoju narud&#382;bu.</p></text>
  </item>
  <item id="VodBuyMoviePIN">
    <text id="en-US"><span class="nNotify">The movie costs $price.</span><br/><p class="nInfoText">Please confirm your order.</p></text>
    <text id="de"><span class="nNotify">Der Film $movieName kostet $price.</span><br/><p class="nInfoText">Bitte bestätigen Sie mit Ihrem Fingerabdruck.</p></text>
    <text id="hr"><span class="nNotify">Film iznosi $price.</span><br/><p class="nInfoText">Molimo vas da potvrdite svoju narud&#382;bu.</p></text>
  </item>
  <item id="VodCancelBuy">
    <text id="en-US"><span class="nOk">Order cancelled.</span></text>
    <text id="de"><span class="nOk">Der Bestellvorgang wurde abgebrochen.</span></text>
    <text id="hr"><span class="nOk">Order cancelled.</span></text>
  </item>
  <item id="VodConfirmBuy">
    <text id="en-US"><span class="nOk">Movie purchased.</span><br/><p class="nInfoText">Select Play to start.</p></text>
    <text id="de"><span class="nOk">Bestellvorgang von $movieName war erfolgreich.</span><br/><p class="nInfoText">Wählen Sie Play, um den Film zu starten.</p></text>
    <text id="hr"><span class="nOk">Kupit film.</span><br/><p class="nInfoText">Odaberite play za start.</p></text>
  </item>

  <item id="RecordAddToList">
    <text id="en-US"><span class="nOk">$recordingName<br/>is scheduled for recording.</span></text>
    <text id="de"><span class="nOk">$recordingName<br/>wird aufgezeichnet".</span></text>
    <text id="hr"><span class="nOk">$recordingName<br/>is scheduled for recording.</span></text>
  </item>
